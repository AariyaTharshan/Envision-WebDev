{
  "name": "react-components",
  "private": true,
  "version": "0.0.0",
  "main": "./Electron/main.js",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "postinstall": "node electron/install-python-deps.js",
    "electron-dev": "cross-env NODE_ENV=development electron .",
    "prepare-python": "node scripts/prepare-build.js",
    "electron-build": "npm run prepare-python && vite build && cross-env NODE_ENV=production electron-builder"
  },
  "dependencies": {
    "lucide-react": "^0.462.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.4.0",
    "react-router-dom": "^7.1.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "eslint": "^9.15.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "vite": "^6.0.1"
  },
  "build": {
    "appId": "com.chennaimetco.Envision",
    "productName": "Envision",
    "files": [
      "dist/**/*",
      "src/**/*",
      "Electron/**/*",
      "node_modules/**/*",
      "backend/**/*",
      "package.json",
      "main.js"
    ],
    "extraResources": [
      {
        "from": "backend/dist",
        "to": "backend/dist",
        "filter": ["camera_server.exe"]
      }
    ],
    "directories": {
      "buildResources": "assets",
      "output": "dist"
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ]
    },
    "extends": null
  }
}
